{
  "name": "minifetch-api",
  "version": "0.0.1",
  "type": "module",
  "bugs": {
    "url": "https://github.com/Niche-Networks/minifetch-api/issues"
  },
  "homepage": "https://minifetch.com/ ",
  "repository": {
    "type": "git",
    "url": "git@github.com:Niche-Networks/minifetch-api.git"
  },
  "scripts": {
    "dev:basedev:check": "tsx index.ts --env=dev --endpoint=check",
    "dev:basedev:metadata": "tsx index.ts --env=dev --endpoint=metadata",
    "dev:svmdev:check": "tsx index.ts --env=dev --endpoint=check",
    "dev:svmdev:metadata": "tsx index.ts --env=dev --endpoint=metadata",
    "format": "prettier -c .prettierrc --write \"**/*.{ts,js,cjs,json,md}\"",
    "format:check": "prettier -c .prettierrc --check \"**/*.{ts,js,cjs,json,md}\"",
    "lint": "eslint . --ext .ts --fix",
    "lint:check": "eslint . --ext .ts"
  },
  "dependencies": {
    "@x402/evm": "^2.2.0",
    "@x402/fetch": "^2.2.0",
    "@x402/svm": "^2.2.0",
    "dotenv": "^16.4.7",
    "viem": "^2.23.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "3.5.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
